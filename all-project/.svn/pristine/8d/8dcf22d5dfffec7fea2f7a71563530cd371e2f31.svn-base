package saas.winbons.framework.sql;

/**
 * @ClassName: FilterType
 * @author star.ye
 * @date 2014年6月28日 上午9:56:18
 */

public enum FilterType {

	EQ("="), NEQ("<>"), GT(">"), GTE(">="), IN("in"), NIN("not in"), 
	EXIST("exists"), NEXIST("not exists"), LT("<"), LTE("<="), 
	LIKE("like"), ISNULL("is null"), ISNOTNULL("is not null"),
	BETWEEN("between");

	public String value;

	private FilterType(String value) {
		this.value = value;
	}
}
