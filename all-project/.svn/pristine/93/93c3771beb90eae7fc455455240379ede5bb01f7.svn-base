package saas.winbons.dubbo.api;

import java.io.Serializable;

public class ContextDTO implements Serializable  {
	
	private static final long serialVersionUID = -6135316431250202197L;
	private Long dbId;
	private Long userId;
	private boolean admin;
	private boolean mobileRequest;
	private Long currentOwnerId;
	private String userName;
	private Long companyId;
	private Integer serviceType;
	private String loginId;
	
	
	
	public ContextDTO(){}
	
	public ContextDTO(Long dbId, Long userId,boolean admin,boolean mobileRequest,Long currentOwnerId) {
		this.dbId = dbId;
		this.userId = userId;
		this.admin = admin;
		this.mobileRequest = mobileRequest;
		this.currentOwnerId = currentOwnerId;
	}
	public Long getDbId() {
		return dbId;
	}
	public void setDbId(Long dbId) {
		this.dbId = dbId;
	}
	public Long getUserId() {
		return userId;
	}
	public void setUserId(Long userId) {
		this.userId = userId;
	}

	public boolean isAdmin() {
		return admin;
	}

	public void setAdmin(boolean admin) {
		this.admin = admin;
	}

	public boolean isMobileRequest() {
		return mobileRequest;
	}

	public void setMobileRequest(boolean mobileRequest) {
		this.mobileRequest = mobileRequest;
	}

	public Long getCurrentOwnerId() {
		return currentOwnerId;
	}

	public void setCurrentOwnerId(Long currentOwnerId) {
		this.currentOwnerId = currentOwnerId;
	}

	public String getUserName() {
		return userName;
	}

	public void setUserName(String userName) {
		this.userName = userName;
	}

	public Long getCompanyId() {
		return companyId;
	}

	public void setCompanyId(Long companyId) {
		this.companyId = companyId;
	}

	public Integer getServiceType() {
		return serviceType;
	}

	public void setServiceType(Integer serviceType) {
		this.serviceType = serviceType;
	}

	public String getLoginId() {
		return loginId;
	}

	public void setLoginId(String loginId) {
		this.loginId = loginId;
	}
	
}
